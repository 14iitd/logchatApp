<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/btn_image"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/teal_200"
    android:orientation="vertical">
<!--    tools:context=".LogInActivity">-->


  <ImageView
      android:id="@+id/imageView1"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_marginTop="200dp"
      android:contentDescription="@string/image_one"
      app:srcCompat="@drawable/log_in" />

  <ImageView
      android:id="@+id/imageView8"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:contentDescription="@string/image_thr"
      app:srcCompat="@drawable/dot" />

  <ImageView
      android:id="@+id/imageView9"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:contentDescription="@string/image_one"
      app:srcCompat="@drawable/log_r" />

  <ImageView
      android:id="@+id/imageView10"
      android:layout_width="match_parent"
      android:contentDescription="@string/image_two"
      android:layout_height="wrap_content"
      app:srcCompat="@drawable/bor" />

  <ImageButton
      android:id="@+id/image_log"
      android:layout_width="match_parent"
      android:layout_height="65dp"
      android:layout_marginStart="30dp"
      android:layout_marginTop="70dp"
      android:layout_marginEnd="30dp"
      android:background="@drawable/log_b"
      android:contentDescription="@string/button_log" />




</LinearLayout>